@model Tto

@{
    ViewData["Title"] = "Create";
}

<h1>Tto Oluşturma Sayfası</h1>

<h4>Tto</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TtoName" class="control-label"></label>
                <span asp-validation-for="TtoName" class="text-danger field-validation-error"></span>
                <input asp-for="TtoName" class="form-control" />
               
            </div>
            <div class="form-group">
                <label asp-for="TtoContent" class="control-label"></label>
                <input asp-for="TtoContent" class="form-control" />
                <span asp-validation-for="TtoContent" class="text-danger"></span>
            </div>
            <div class="col-md-10">
                <p>Dosyayı seçiniz : </p>
                <input type="file" name="file" />
            </div>
            <div class="form-group">
                <input type="submit" value="Ekle" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Listeye dön</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
